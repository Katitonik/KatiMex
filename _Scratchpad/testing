from collections import namedtuple
dish = namedtuple('Dish', ['name', 'spiciness', 'size'])
from enum import IntEnum
class spiciness(IntEnum):
    Mild = 0
    Hot = 1
    Very_Hot = 2
    Why = 3
from enum import IntEnum
class size(IntEnum):
    S = 0
    M = 1
    L = 2
    How = 3
Mole = dish('Mole', spiciness.Mild, size.M)
dish
mole
Mole
Employee = namedtuple('Employee', ('ID', 'name', 'role', 'psswdhash'))
Alex = Employee('101', 'Alex', 'Cashier', psswdhash=hash("I cant remember!"))
alex
Alex
